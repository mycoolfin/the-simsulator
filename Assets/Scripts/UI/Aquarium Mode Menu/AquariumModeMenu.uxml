<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement style="flex-grow: 0; background-color: rgb(254, 207, 186); width: 30px; flex-shrink: 0; flex-direction: row; align-self: auto; align-items: center; -unity-text-align: upper-left; justify-content: center; position: absolute; right: 0; border-bottom-left-radius: 5px; height: 30px; -unity-text-outline-color: rgba(254, 207, 186, 0);">
        <ui:Button text="&lt;&lt;" display-tooltip-when-elided="true" name="menu-open" style="flex-shrink: 0; width: 30px; flex-grow: 0; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; background-color: rgba(188, 188, 188, 0); border-left-color: rgba(149, 149, 149, 0); border-right-color: rgba(149, 149, 149, 0); border-top-color: rgba(149, 149, 149, 0); border-bottom-color: rgba(149, 149, 149, 0); -unity-font-style: normal; align-items: center;" />
    </ui:VisualElement>
    <ui:VisualElement name="menu" style="flex-grow: 0; background-color: rgb(255, 215, 167); align-self: flex-end; min-height: auto; min-width: auto; opacity: 1; display: flex; visibility: visible; overflow: visible; width: 450px; height: auto; transition-property: left; transition-duration: 0.3s; transition-timing-function: ease; transition-delay: 0s; left: 0;">
        <ui:VisualElement name="menu-header" style="flex-grow: 0; background-color: rgb(255, 207, 186); width: auto; flex-shrink: 0; flex-direction: row; align-self: auto; align-items: center; height: 30px;">
            <ui:Button text="&gt;&gt;" display-tooltip-when-elided="true" name="menu-close" focusable="true" style="flex-shrink: 0; width: 30px; flex-grow: 0; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; background-color: rgba(188, 188, 188, 0); border-left-color: rgba(149, 149, 149, 0); border-right-color: rgba(149, 149, 149, 0); border-top-color: rgba(149, 149, 149, 0); border-bottom-color: rgba(149, 149, 149, 0); -unity-font-style: normal; position: relative; left: 0; top: 0; align-items: auto; justify-content: flex-start; height: auto; -unity-background-scale-mode: stretch-to-fill; overflow: hidden;" />
            <ui:Label tabindex="-1" text="Aquarium Mode" display-tooltip-when-elided="true" style="flex-grow: 1; align-items: auto; align-self: center; -unity-text-align: upper-center; -unity-font-style: normal;" />
        </ui:VisualElement>
        <ui:ScrollView mouse-wheel-scroll-size="100">
            <ui:VisualElement name="environment" focusable="false" style="flex-grow: 0; background-color: rgba(0, 0, 0, 0); visibility: visible; opacity: 1; display: flex;">
